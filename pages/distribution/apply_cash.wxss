/* apply_cash.wxss */
.section {padding:0;margin:0;border:none;background:#fff;}
.cashSection {padding:20rpx 25rpx 10rpx;}
.cashSection .sectionItem {display:flex;align-items:center;padding:20rpx 0;}
.cashSection .sectionItem:first-child {border-bottom:1px solid #e6e6e6;}
.cashSection .sectionItem .leftLabel {flex:1;font-size:24rpx;color:#333;}
.cashSection .sectionItem .rightBox {flex:3;font-size:36rpx;color:#F6640C;text-align:left;}
.cashSection .sectionItem .itemDesc {font-size:24rpx;color:#999;}
.brokerageSection {background:transparent;}
.brokerageSection .sectionItem:first-child {border-bottom:none;}
.applySection {padding:0 25rpx;margin-bottom:20rpx;}
.applySection .sectionItem {display:flex;align-items:center;padding:25rpx 0;border-bottom:1px solid #e6e6e6;font-size:24rpx;color:#666;}
.applySection .sectionItem:last-child {border-bottom:none;}
.applySection .sectionItem .leftLabel {flex:1;}
.applySection .sectionItem .rightBox {flex:3;color:#999;text-align:right;padding-right:50rpx;}
.applySection .sectionItem .rightBox input {font-size:24rpx;color:#666;text-align:right;}
.bankChoose{display: flex;justify-content: flex-end;align-items: center;}
.icon-camera{margin-left: 16rpx;}
.moneySection {padding:0 25rpx;}
.moneySection .sectionItem {display:flex;align-items:center;padding:20rpx 0;font-size:24rpx;color:#333;}
.moneySection .sectionItem.sectionInput {font-size:36rpx;color:#333;}
.moneySection .sectionItem.sectionInput text {width:50rpx;}
.moneySection .sectionItem.sectionInput input {font-size:36rpx;color:#333;}
.applyButton {width:666rpx;height:80rpx;background:#FDA814;line-height:80rpx;text-align:center;font-size:36rpx;color:#fff;border-radius:10rpx;margin:30rpx auto;}
.btn {width: 80%;padding: 20rpx 0;border-radius: 10rpx;text-align: center;margin: 40rpx 10%;background: #000;color: #fff;}  
.drawer_screen {width: 100%;height: 100%;position: fixed;top: 0;left: 0;z-index: 1000;background: #000;opacity: 0.5;overflow: hidden;}
.drawer_box {width: 650rpx;overflow: hidden;position: fixed;top: 50%;left: 0;z-index: 1001;background: #FAFAFA;margin: -150px 50rpx 0 50rpx;border-radius: 3px;}
.drawer_title{padding:25rpx;font: 32rpx "microsoft yahei";text-align: center;color:#333;}
.drawer_content {height: 420rpx;overflow-y: scroll; /*超出父盒子高度可滚动*/padding:0 25rpx;}
.drawer_box .radio-group .radio {display:flex;align-items:center;padding:16rpx 0;font-size:28rpx;border-bottom:1px solid #e6e6e6;color:#666;}
.drawer_box .radio-group .radio {border-bottom:none;}
.drawer_box .radio-group .radio text {flex:5;}
.drawer_box .radio-group .radio radio {flex:1;text-align:right;}
.btn_ok{ padding: 10px; font: 36rpx "microsoft yahei"; text-align: center; border-top: 1px solid #E8E8EA; color: #3CC51F; }
.applySucPop {position:fixed;display:none;top:0;left:0;right:0;bottom:0;z-index:100;background:#f6f6f6;}
.applySucPop .popSign{height:200rpx;width:200rpx;margin:0 auto;margin-top:300rpx;}
.applySucPop .popStatu{color:#F6640D;text-align:center;font-size:40rpx;padding:25rpx 0;margin-bottom:25rpx;}
.applySucPop .popInfo{display:flex;flex-direction:column;font-size:28rpx;color:#333;align-items:center;margin-bottom:30rpx;}
.applySucPop .popLink {display:flex;align-items:center;}
.applySucPop .popLink navigator {color:#FDA815;text-align:center;padding-bottom:5rpx;border-bottom:1px solid #FDA815;margin:0 auto;font-size:28rpx;}
.applySucPop .popLink navigator .iconfont {font-size:28rpx;margin-right:0rpx;}


