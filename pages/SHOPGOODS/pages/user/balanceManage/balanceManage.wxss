/* pages/SHOPGOODS//pages/user/balanceManage/balanceManage.wxss */
.mymoney {
  font-size: 26rpx;
}

.mymoney .header {
  width: calc(100% - 200rpx);
  height: 110rpx;
  border-top: 1rpx solid #e1e1e1;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: white;
  color: #333;
  padding: 0 100rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 100;
}

.mymoney .header  .item {
  font-size: 30rpx;
  font-weight: 600;
  height: 70rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.line {
  width: 80rpx;
  height: 6rpx;
  background-color: white;
}

.mymoney .header .active {
  color: #ff5c1c;
}

.line_active {
  background-color: transparent;
}
/* 账户明细 */
.list .title {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  background-color: #f2f3f4;
  color: #333;
  font-weight: 500;
}

.list .date {
  width: 100%;
  height: 120rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  background-color: #f9f9f9;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.list .date .month {
  width: max-content;
  padding: 5rpx 25rpx;
  height: 50rpx;
  background-color: #fff;
  border-radius: 25rpx;
  border: solid 1px #efefef;
  font-size: 24rpx;
  color: #333;
  display: flex;
  align-items: center;
}

.list .date .month image {
  width: 14rpx;
  height: 10rpx;
  background-color: #bbb;
  margin-left: 5rpx;
}
.total text{
  color: #bbb;
  font-size: 26rpx;
}
.date .icon-xia{
  font-size: 14rpx;
  margin-left: 10rpx;
  color: #bbb;
}
/* 列表 */
.list-item{
  padding: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  background-color: #fff;
  border-bottom: 1px solid #f9f9f9;
}
.li-left{
  display: flex;
  flex-direction: column;
  color: rgb(16, 16, 16);
  font-size: 24rpx;
}
.li-name{
  font-size: 28rpx;
}
.li-product-name{
  width: 440rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: sub;
}
.li-tip{
  color: rgb(250, 0, 40);
  background-color: rgba(250, 0, 40,.2);
  padding: 2rpx 10rpx;
  border-radius: 22rpx;
  margin-left: 10rpx;
  font-size: 24rpx;
}
.li-time{
  margin: 6rpx 0;
}
.order-copy{
  margin-left: 6rpx;
  color: rgba(250, 0, 40, 1);
  text-decoration: underline;
}
.li-right{
  font-size: 32rpx;
  font-weight: 600;
  color: rgba(250, 0, 40, 1);
}
/* 底部提现按钮 */
.submit-btn{
  background-color: #fff;
  padding: 40rpx;
  text-align: left;
  margin-bottom: 20rpx;
}
.txbtn{
  margin: 0 auto;
  width: 80%;
  height: 80rpx;
  color: white;
  line-height: 80rpx;
  text-align: center;
  font-size: 36rpx;
  border-radius: 10rpx;
}
.coupon {
  margin-top: 20rpx;
  width: 100%;
  font-size: 24rpx;
  background: white;
  padding: 35rpx;
  box-sizing: border-box;
  background:#f7f7f7;
}
.message {
  margin: 20rpx 0;
  width: 100%;
  font-size: 24rpx;
  background: white;
  padding: 35rpx;
  box-sizing: border-box;
}

.message  .title {
  font-size: 28rpx;
  font-weight: 500;
  color: #333;
  margin-bottom: 20rpx;
}
/**日期选择**/

.chosedate {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 625rpx;
  background: white;
}

.upDown{
  animation: upDown 800ms ease;
}

@keyframes upDown{
  0%{
    bottom: -100%;
  }
  100%{
    bottom:0%;
  }
}

.chosedate .title {
  height: 100rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #f2f2f2;
  font-size: 26rpx;
  color: #333;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.chosedate .title .btntrue {
  font-size: 26rpx;
}

.showdate {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color: #ccc;
}

.showdate .time {
  display: block;
  width: 278rpx;
  text-align: center;
  padding-bottom: 15rpx;
  border-bottom: 1px solid #ccc;
}

.picker-view {
  text-align: center;
}

.picker-view-column view {
  text-align: center;
}
.isover{
  color: #333333;
  padding: 50rpx 0;
}

