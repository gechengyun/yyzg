/* 充值类型 */
/* 方式1 */
.balance-mode{
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
  padding: 0 30rpx;
}
.balance-mode-item{
  background-color: #f7f7f7;
  border-radius: 12rpx;
  margin-right: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 210rpx;
  margin-bottom: 30rpx;
  padding: 20rpx 0;
  box-sizing: border-box;
  border: 1px solid #f7f7f7;
}
.balance-mode-item:nth-child(3n){
  margin-right: 0;
}
.balance-mode-item-b{
  color: #9a9a9a;
}
.balance-ipt{
  padding: 0 30rpx;
  background-color: #fff;
  text-align: center;
}
.balance-ipt input{
  padding: 20rpx 30rpx;
  background-color: #f7f7f7;
}
.balance-ipt::after{
  content: '';
  width: 100%;
  display: block;
  border-bottom: 1px solid #bbb;
  margin-top: 30rpx;
}
.balance-tips{
  text-align: center;
  padding: 20rpx 0;
  background-color: #fff;
}
.cashmoney .balance-tips{
  text-align: left;
}
/* 方式2 */
.cashmoney {
  width: 100%;
  padding-left: 40rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.cashmoney  .money {
  width: 100%;
  height: 140rpx;
