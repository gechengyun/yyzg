  font-size: 24rpx;
  margin-top: 3rpx;
  height: 44rpx;
  line-height: 44rpx;
  background-color: #f9f9f9;
}

.circle-buy-item-title {
  flex: 1;
  background-color: #e6e6e6;
  border: 1rpx solid #e6e6e6;
}

.circle-buy-item-desc {
  flex: 2;
}

.circle-buy-send-count {
  padding: 15rpx 20rpx;
  height: 20rpx;
  line-height: 20rpx;
}

.circle-buy-send-count text {
  font-size: 26rpx;
  color: #353535;
}

