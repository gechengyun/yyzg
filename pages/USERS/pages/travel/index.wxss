/* index.wxss */
.container {background:#f6f6f6;}
.swiper-container{ position: relative; } 
.swiper-container .swiper { height: 600rpx; } 
.swiper-container .swiper .img{ width: 100%; height: 100%; } 
.swiper-container .dots{ position: absolute; right: 40rpx; bottom: 20rpx; display: flex; justify-content: center; } 
.swiper-container .dots .dot{ margin: 0 10rpx; width: 28rpx; height: 28rpx; background: #fff; border-radius: 50%; transition: all .6s;font: 300 18rpx/28rpx "microsoft yahei"; text-align: center;} 
.swiper-container .dots .dot.active{ background: #f80; color:#fff; }
.swiper-container .bannerPage{position:absolute;right:40rpx;bottom:20rpx;width:100rpx;height:100rpx;border-radius: 50%;background: rgba(0,0,0,.7); color:#fff; }
.swiper-container .bannerPage text{font-size:35rpx; position:absolute;}
.swiper-container .bannerPage text.first{top:14rpx;left:28rpx;}
.swiper-container .bannerPage text.last{bottom:14rpx;right:28rpx;}
.swiper-container .bannerPage text.line{top:26rpx;left:50rpx;border-left:3rpx solid #fff;transform:rotate(45deg);height:50rpx;}

.section {margin-top:0;margin-bottom:24rpx;padding:20rpx;border:none;}
.section .infoTitle {font-size:26rpx;color:#363636;padding-bottom:20rpx;font-weight:bold;border-bottom:1px solid #F1F1F1;}
.section .infoSection {padding:26rpx 0 8rpx 0;display: block;}
.detailsCentent .productName wx-view.productH2 {font-weight:bold;}
.detailsCentent .productInfo {border-bottom:1px solid #f1f1f1;}
.detailsCentent .productInfo .productPrice>view {float:left;width:100%;font-size:36rpx;color:#F6640C;}
.detailsCentent .productInfo .productPrice > wx-view:last-child {margin-left:0rpx;}
.detailsCentent .productInfo .productPrice>view text.originalPrice{margin-left:10rpx;font-size:28rpx;color:#999;text-decoration:line-through;}
.detailsCentent .productInfo .productFX {float:right;font-size:28rpx;}
.detailsCentent .productInfo .productFX navigator{display: block;width:100%;height:100%;}
.detailsCentent .productInfo .productFX view{color:#F6640C;border:1px solid #F6640C;border-radius:4rpx;width:120rpx;height:50rpx;box-sizing:border-box;text-align:center;line-height:46rpx;box-shadow:0px 0px 1px #F6640C;}
.detailsCentent .productLimit {font-size:24rpx;color:#757575;padding-top:24rpx;padding-bottom:10rpx;}
.detailsCentent .productLimit view {display:inline-block;margin-right:20rpx;width:80rpx;height:36rpx;border-radius:4rpx;color:#fff;background:#FDA814;text-align: center;line-height: 36rpx;}
.detailsCentent .productLimit text.detail{float:right;font-size:26rpx;}
.discountInfo .infoSection .discountItem {padding:5rpx 0;font-size:24rpx;}
.discountInfo .infoSection .discountItem .itemName {display:inline-block;width:125rpx;height:40rpx;box-sizing:border-box;border:2rpx solid #FDA814;color:#FDA814;font-size:24rpx;line-height:36rpx;text-align: center;border-radius:5rpx;margin-right:20rpx;}
.activityInfo .infoSection .activityItem {padding:5rpx 0;font-size:24rpx;}
.activityInfo .infoSection .activityItem navigator {display:block;}
.activityInfo .infoSection .activityItem .itemName {display:inline-block;width:80rpx;height:36rpx;box-sizing:border-box;background:#FDA814;color:#fff;font-size:24rpx;line-height:36rpx;text-align: center;border-radius:5rpx;margin-right:20rpx;}
.activityInfo .infoSection .activityItem:nth-child(even) .itemName {background:#FF6868;}
.activityInfo .infoSection .activityItem text.arrow {font-size:26rpx;float:right;color:#999;}
.section.leaveInfo {padding-bottom:10rpx;}
.leaveInfo .infoSection .leaveList {display:flex;flex-wrap: wrap;} 
.leaveInfo .infoSection .leaveItem {width:160rpx;height:90rpx;padding:6rpx;display:flex;flex-direction:column;box-sizing:border-box;border:2rpx solid #E2E2E2;color:#666;font-size:28rpx;line-height:88rpx;text-align: center;border-radius:15rpx;margin-right:16rpx;margin-bottom:16rpx;}
.leaveInfo .infoSection .leaveItem navigator {width:100%;height:100%;display:flex;flex-direction:column;}
.leaveInfo .infoSection .leaveItem:nth-child(4n+4) {margin-right:0;}
.leaveInfo .infoSection .leaveItem text {width:100%;height:50%;line-height:45rpx;}
.leaveInfo .infoSection .leaveItem text.leavePrice {font-size:28rpx;color:#F66712;}
.routeInfo .routeDetail {float:right;font-size:24rpx;color:#999;font-weight:normal;}
.routeInfo .routeDetail navigator {display: block;width: 100%;height:100%;}
.routeInfo .routeDetail text {margin-left:10rpx;font-size:26rpx;}
.routeInfo .routeList .routeItem {padding:10rpx 0;font-size:26rpx;color:#666;}
.routeInfo .routeList .routeItem .itemTime{display:inline-block;margin-right:25rpx;background:#FDA814;border-radius:50%;height:64rpx;width:64rpx;line-height:62rpx;text-align:center;font-size:28rpx;color:#fff;font-weight:bold;}
/* 分类说明展示（用户评价） */
.productInfoList {background:#fff;padding:0;border-top:1px solid #e6e6e6;}
.productInfoList .ulList .navItem {flex:1;font-size:26rpx;color:#666;text-align:center;padding:26rpx 0;border-right:1px solid #e6e6e6;}
.productInfoList .ulList .navItem.active {color: #fff;background: #ff6634;}
.productInfoList .productInfo {padding:0 25rpx;}
.productInfoList .productInfo .artSection {display:none;}
.productInfoList .productInfo .artSection.active {display:block;}
.productInfoList .productInfo .artSection .infoSection {padding:26rpx 0;display: block;}
.productInfoList .productInfo .featureSection .featureItem,
.productInfoList .productInfo .noticeSection .noticeItem {padding:5rpx 0;line-height:40rpx;font-size:24rpx;color:#999;}
.productInfoList .productInfo .featureItem .itemName,
.productInfoList .productInfo .noticeSection .noticeItem .itemName {width:125rpx;height:40rpx;box-sizing:border-box;border:2rpx solid #FDA814;color:#FDA814;font-size:24rpx;line-height:36rpx;text-align: center;border-radius:5rpx;margin-right:20rpx;padding:0 10rpx;}
.productInfoList .productInfo .productEvaluate {background:#fff;}
.productInfoList .productInfo .productEvaluate .title {padding:0 20rpx;font-size:24rpx;color:#333;line-height:60rpx;border-bottom:1rpx solid #e6e6e6;}
.productInfoList .productInfo .productEvaluate .title .span {width:350rpx;height:60rpx;display:inline-block;vertical-align:top;}
.productInfoList .productInfo .productEvaluate .title .span text {color:#ff6634;}
.productInfoList .productInfo .productEvaluate .title .span:nth-child(2) {text-align:right;}
.productInfoList .productInfo .productEvaluate .evaluateTitle {padding:20rpx 10rpx;background:#f9f9f9;}
.productInfoList .productInfo .productEvaluate .evaluateTitle view {float:left;width:150rpx;margin:0 10rpx;font-size:24rpx;color:#666;line-height:48rpx;text-align:center;border:1rpx solid #e6e6e6;border-radius:10rpx;box-sizing:border-box;}
.productInfoList .productInfo .productEvaluate .evaluateTitle view.active {color:#ff6634;border:1rpx solid #ff6634;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList {border-top:1rpx solid #e6e6e6;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view {padding:20rpx;border-bottom:1rpx solid #e6e6e6;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view:last-child {border:0;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view .p {line-height:60rpx;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view .p view {width:350rpx;display:inline-block;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view .p view image {width:60rpx;height:60rpx;margin-right:20rpx;display:inline-block;vertical-align:top;overflow:hidden;border:1rpx solid #e6e6e6;border-radius:50%;box-sizing:border-box;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view .p text:nth-child(2) {max-width:250rpx;font-size:28rpx;color:#333;text-overflow:ellipsis;white-space:nowrap;display:inline-block;vertical-align:top;overflow:hidden;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view .p text:nth-child(3) {width:60rpx;margin-top:18rpx;font-size:22rpx;font-weight:normal;color:#ff6634;line-height:24rpx;text-align:center;display:inline-block;vertical-align:top;border:1rpx solid #ff6634;border-radius:1rpx;margin-left:6rpx;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view .p view:nth-child(2) {float:right;width:240rpx;font-size:22rpx;color:#999;text-align:right;display:inline-block;vertical-align:top;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList>view .p:nth-child(2) {padding:12rpx 0;font-size:24rpx;color:#333;line-height:36rpx;}
.productInfoList .productInfo .productEvaluate .evaluateTitleList  .evaluateImg view {float:left;width:119rpx;height:120rpx;margin:10rpx;overflow:hidden;border:1rpx solid #e6e6e6;}
/* 底部按钮 */
