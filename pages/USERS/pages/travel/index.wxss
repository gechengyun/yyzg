.consultPop {display:none;position:fixed;z-index:15;bottom:90rpx;left:0;right:0;background:#fff;}
.consultPop .popHeader {padding:30rpx;font-size:24rpx;color:#999;border-bottom:2rpx solid #EAEAEA;}
