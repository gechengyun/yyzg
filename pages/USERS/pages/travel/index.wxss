.dateSection .dateList .dateItem .manPrice {font-size:22rpx;display:none;}
.dateSection .dateList .dateItem {background:#FDA814;color:#fff;}
.dateSection .dateList .dateItem.active .manPrice {display:block;}
.dateSection .dateList .dateItem.active {background:#F8823F;color:#fff;}
.dateSection .dateList .dateItem.active .manPrice {display:block;}
