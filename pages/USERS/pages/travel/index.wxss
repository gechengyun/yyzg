.shoppingCatFooter .ulList .shoppingCatButton {font-size:26rpx;color:#fff;text-align:center;background:#ff9e40;border-right:1rpx solid #ee8016;flex:1;height:90rpx;line-height:90rpx;}
.shoppingCatFooter .ulList .shoppingCatButton.add_cart {width:200rpx;background:#ff5c5c;border:0;} 
