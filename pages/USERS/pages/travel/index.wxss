/* 旅游下单页面 */
.container {background:#f6f6f6;}
.section {margin-top:0;margin-bottom:24rpx;box-sizing: border-box;width:100%;border:none;}
.orderTravel .section {padding: 0;}
.section .sectionTitle{border-bottom:1px solid #E7E7E7;font-size:26rpx;font-weight:bold;padding:30rpx;text-align:left;color:#333;}
.dateSection .monthNav {display:flex;flex-wrap:wrap;background:#fafafa;width:100%;font-size:26rpx;padding:10rpx 0;color:#666;}
