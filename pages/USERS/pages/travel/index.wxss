.peopleSection .peopleList .peopleItem .leftLabel {flex:1;font-size:26rpx;color:#666;line-height: 50rpx;}
.peopleSection .peopleList .peopleItem .leftLabel .itemPrice {color:#F8823F;}
.peopleSection .peopleList .peopleItem .rightBox {flex:1;font-size:26rpx;text-align: right;}
.peopleSection .peopleList .peopleItem .rightBox text.iconfont {font-size:38rpx;color:#fda815;}
.peopleSection .peopleList .peopleItem .rightBox text.icon-jianhao {font-size:44rpx;}
.peopleSection .peopleList .peopleItem .rightBox text.itemNum {margin: 0 15rpx;position: relative;top:-4rpx;}
.contactSection .contactList, .travelerSection .travelerList {padding:0 30rpx;}
.contactSection .contactList .listItem, .travelerSection .travelerList .listItem {display:flex;flex-wrap: nowrap;font-size:26rpx;line-height:48rpx;color:#666;padding:36rpx 0;border-bottom: 1px solid #e6e6e6;}
.contactSection .contactList .listItem:last-child, .travelerSection .travelerList .listItem:last-child {border-bottom:none;}
.contactSection .contactList .listItem .leftLabel, .travelerSection .travelerList .listItem .leftLabel {flex:1;}
.contactSection .contactList .listItem .rightBox, .travelerSection .travelerList .listItem .rightBox {flex:3;}
.agreementSection {margin-bottom:40rpx;background:transparent;}
.agreementSection .agreementBox {padding:10rpx 30rpx;font-size:26rpx;color:#999;}
.agreementSection .agreementBox.active{color:#333;}
.agreementSection .agreementBox .icon-duihao1{position: relative;}
.agreementSection .agreementBox .icon-duihao1.active{color: #fda815;}
.agreementSection .agreementBox text.iconfont{font-size:26rpx;margin-right:10rpx;}
.agreementSection .agreementBox text.agreement{color:#FDA814;}
.shoppingCatFooter .ulList .shoppingCatButton {font-size:26rpx;color:#fff;text-align:center;background:#ff9e40;border-right:1rpx solid #ee8016;flex:1;height:90rpx;line-height:90rpx;}
.shoppingCatFooter .ulList {z-index:5;}
.shoppingCatFooter .ulList .shoppingCatButton{font-size:32rpx;border-right:none;}
.shoppingCatFooter .ulList .shoppingCatButton.active{background: #ff9e40;}
.shoppingCatFooter .ulList .shoppingCatButton.reserPrice{background:#fff;color:#333;}
.shoppingCatFooter .ulList .shoppingCatButton.reserPrice .price {color:#F6640C;}

