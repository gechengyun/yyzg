/* pages/LIVEVIDEO//pages/liveVideo/liveVideoDetail.wxss */
page, .container {
  margin: 0;
  padding: 0;
}
/* 显示不全方法 */
.after-box{
  display: flex;
}
.after-box::after{
  content: '';
  display: block;
  width: 20rpx;
  height: 1px;
}
.live-player {
  width: 100%;
  height: 100vh;
  vertical-align: middle;
  position: absolute;
  left: 0;
  top: 0;
  overflow: hidden;
}

/* 直播页信息 */

.live-playered {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
  width: 100%;
  min-height: 100%;
  background-color: rgba(0, 0, 0, 0);
}

.img-cover {
  width: 104%;
  height: 104%;
  vertical-align: middle;
  filter: blur(3px);
  margin: -2% 0 0 -2%;
}

/* 直播者信息 */

.user-mes {
  position: absolute;
  left: 110rpx;
  top: 52rpx;
  z-index: 1;
  background-color: rgba(0, 0, 0, 0.38);
  color: #fff;
  font-size: 26rpx;
  margin-top: -3rpx;
  padding: 3rpx 14rpx 3rpx 3rpx;
  border-radius: 66rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  overflow: hidden;
}

.user-avatar {
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 8rpx;
}

.user-content {
  margin-right: 12rpx;
}

.user-content cover-view:first-child {
  max-width: 120rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.look-num {
  font-size: 24rpx;
  margin-top: 4rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.look-nums {
  margin-right: 10rpx;
  flex-shrink: 0;
}

.follow {
  color: #fff;
  font-size: 24rpx;
  border-radius: 26rpx;
  position: relative;
  width: 84rpx;
  height: 52rpx;
  line-height: 52rpx;
  /* margin-top: -4rpx; */
}

.followed {
  background-color: rgb(195, 195, 195);
  width: 110rpx;
}

.follow-img {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.follow-contnet {
  position: absolute;
  left: 18rpx;
  top: 12rpx;
}

.getPhoneNum {
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1;
  width: 130rpx;
  height: 70rpx;
  opacity: 0;
}

/* 关闭直播 */

.live-close {
  position: fixed;
  top: 55rpx;
  left: 30rpx;
  z-index: 3;
}

.live-close navigator {
  display: block;
}

.close-live-img {
