/* pages/SHOPKEEPER//pages/userPage/userList.wxss */
/* Tab */
page{
  background-color: #fff;
}
.list-tab{
  width: 100%;
  height: 108rpx;
  padding: 0 60rpx;
  box-sizing: border-box;
  position: fixed;
  z-index: 5;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: 1px solid rgb(238,238,238);
  background-color: #fff;
}
.tab-active{
  font-weight: 700;
  border-bottom: 6rpx solid #bbb;
  box-sizing: border-box;
}
.tab-item{
  height: 108rpx;
  line-height: 108rpx;
  font-size: 30rpx;
  color: rgb(51,51,51);
}
/* List */
.list-content{
  padding: 0 20rpx;
  background-color: #fff;
}
.list-item{
  padding: 15rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list-avatar{
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  font-size: 0;
  vertical-align: middle;
}
.list-name{
  vertical-align: middle;
  margin-left: 18rpx;
  font-size: 30rpx;
  color: rgb(51,51,51);
}
.list-r{
  background-color: rgb(187,187,187);
  border-radius: 32rpx;
  font-size: 26rpx;
  color: #fff;
  width: 160rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
}
.list-r .iconfont{
  font-size: 36rpx;
  font-weight: 700;
}
.list-r .icon-correct{
  margin-right: 6rpx;
  vertical-align: middle;
}
.list-r .icon-iconfont7{
  font-size: 30rpx;
  margin-right: 10rpx;
}
.list-r .icon-luxian{
  font-weight: 500;
}
/* 空数据 */
.anchor-list-empy{
  color: #5E5E5E;
  font-size: 24rpx;
  text-align: center;
  padding: 50rpx 0;
  margin-top: 25%;
}
.anchor-list-empy image{
  width: 326rpx;
  margin-bottom: 36rpx;
}