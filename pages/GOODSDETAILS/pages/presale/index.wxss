
page{background: #F1F6F9}
.fr{float: right;}
.fl{float: left;}
.presale .b_banner {height: 750rpx;width: 100%;position: relative;}
.presale .b_banner image{height: 100%}
.presale .b_banner span{position: absolute;left: 0;bottom: 40rpx;background-color: #fe6b31;color: #fff;padding: 0 10rpx;font-size: 20rpx;border-radius: 0 5rpx 5rpx 0;height: 38rpx;line-height: 38rpx}

.presale .status {background: #fff}
.presale .status .priceRow {background-color: #FF386E;}
.presale .status .priceRow {height: 104rpx;line-height: 104rpx;text-align: left;overflow: hidden;font-size: 32rpx;color: #fff}
.presale .status .priceRow em{font-size: 40rpx}
.presale .status .priceRow .wrap{padding: 0 20rpx}

.presale .status .statusRow{height: 104rpx;background: #FCEDF1;padding: 0 20rpx}
.presale .status .statusRow .h2{font-size: 24rpx;color:#FF3867;text-align: center; display: block;margin-top: 10rpx}

.presale .status .priceRow.stopRow{background:#959595 }
.presale .status .statusRow.stopRow{background:#BCBCBC;min-width:200rpx;text-align: center;}
.presale .status .statusRow .stopdiv{line-height: 104rpx;height: 104rpx;color: #fff;}

.presale .countDown .wrap {padding: 30rpx 24rpx;padding-top: 0}
.presale .countDown span,.presale .countDown em {display: inline-block;vertical-align: middle;}
.presale .countDown span{font-size: 30rpx;color: #FF3867}
.presale .countDown em{font-size: 26rpx;color: #24A854;margin: 0 10rpx}
.presale .countDown em.text{font-size: 28rpx}







.presale .shareBlock {background: #fff}
.presale .shareBlock .shareBtn{padding: 0;margin: 0;height: 100%;width:150rpx;text-align: center;margin-left: 10rpx;line-height: 1;border-radius: 0;background:none;}
.presale .shareBlock .shareBtn::before,.presale .shareBlock .shareBtn::after{display: none;}
.presale .shareBlock .shareBtn i{font-size: 50rpx;color: #FF3866;position: relative;top: 24rpx}
.presale .shareBlock .shareBtn p{font-size: 24rpx;color: rgb(136,136,136);display: block;margin-top: 40rpx;}
.presale .shareBlock .name{height: 100%}
.presale .shareBlock .name .table{display: table;/*height: 140rpx;*/padding-left: 20rpx;width: 75%;}
.presale .shareBlock .name .t_cell{display: table-cell;vertical-align: middle;font-size: 34rpx;color: #222;display:-webkit-box;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical;
overflow: hidden;padding-top: 10rpx;}
.presale .shareBlock .name .t_cell em{color: #888;font-size: 28rpx;margin-left: 10rpx}
.presale .shareBlock .proData{width: 100%;background: #fff}
.presale .shareBlock .proData .wrap{padding: 0 20rpx;padding-bottom: 10rpx}
.presale .shareBlock .proData span{display: inline-block;width: 30%;text-align: left;color: rgb(136,136,136);font-size: 24rpx}


.presale .attrBlock {background: #fff;padding: 20rpx 0;border-top:20rpx solid #F1F6F9;}
.presale .attrBlock .wrap{padding: 0 20rpx}
.presale .attrBlock span{font-size: 30rpx;}
.presale .attrBlock span.tit{color: rgb(136,136,136);}
.presale .attrBlock span.tit i{margin-right: 5rpx;font-size: 40rpx;color: #828282;position: relative;top: 0;vertical-align: middle;}
.presale .attrBlock span.selectValue{color: #333;font-size: 30rpx}
.presale .attrBlock span.selectValue i{color: rgb(136,136,136) }


.presale .d_desc {background: #fff;border-top:20rpx solid #F1F6F9;}
.presale .d_desc:last-child{margin-bottom: 0}
.presale .d_desc .hdTit{padding: 0 20rpx}
.presale .d_desc .hdTit .h3{font-size: 34rpx;color: #ff3867;text-align: center;line-height: 95rpx;}
.presale .d_desc .thisDesc image{max-width: 100%;display: block;}



.presale .labelBlock {margin: 20rpx 0}
.presale .labelBlock .wrap{padding: 0 20rpx}

.presale .labelBlock .iden{width: 114rpx;height: 127rpx;background-size: cover!important;line-height: 127px;margin-right: 30rpx}
.presale .labelBlock .iden p{font-size:28rpx;color: #fff;text-align: center;display: block;position: relative;top: -5rpx;height: 100%;line-height: 127rpx }
.presale .labelBlock .d_detail{overflow: hidden;}
.presale .labelBlock .d_detail .h2,.presale .labelBlock .d_detail .h3{font-size: 28rpx;color: rgb(51,51,51);display: block;}
.presale .labelBlock .d_detail .h3,.presale .labelBlock .d_detail .gift{margin-top: 15rpx}
.presale .labelBlock .d_detail .h2{margin-top: 15rpx}
.presale .labelBlock .d_detail .red{color:#FF3866 }
.presale .labelBlock .d_detail .gift span{display: inline-block;margin-right: 16rpx;border:1px solid rgb(256,56,103);height: 38rpx;border-radius: 4rpx;font-size: 20rpx;line-height: 38rpx;padding: 0 15rpx}
.presale .labelBlock .d_detail .gift span.span1{border-color: rgb(256,56,103);color: rgb(256,56,103)}
.presale .labelBlock .d_detail .gift span.span2{border-color: rgb(255,108,0);color: rgb(255,108,0)}
.presale .labelBlock .d_detail .gift span.span3{border-color: rgb(4,204,13);color: rgb(4,204,13)}
.presale .labelBlock .d_detail .thisArrow {font-size:15rpx;padding:6rpx;box-sizing:border-box;top:-3px;position:relative;left:10rpx;}
.presale .labelBlock .iden_1{background: url('data:image/png;base64,');}

.presale .labelBlock .iden_2{background: url('data:image/png;base64,')}
.presale .labelBlock .iden_3{background: url('data:image/png;base64,')}

.presale .textDesc .wrap{padding: 20rpx;text-align: left;}
.presale .textDesc .thisText{background: #F1F7FB;padding: 20rpx 20rpx}
.presale .textDesc p{line-height: 1.5;font-size: 28rpx;color: rgb(51,51,51);display: block;}

.presale .d_desc .thisDesc .wrap{padding: 0 20rpx}

.mask{z-index:20;position:fixed;top:0;right:0;bottom:0;left:0;width:100%;background-color:#000;opacity:.7;filter:alpha(opacity=80);-moz-opacity:.8;-khtml-opacity:.8;}
.thisItemWindow{position: fixed;bottom: 0;left: 0;right: 0;background: #fff;z-index: 21}
.thisItemWindow .itemDetails{padding-bottom: 100rpx}
.thisItemWindow .itemDetails .wrap{padding: 0 20rpx}
.thisItemWindow .itemDetails .hd{position: relative;}
.thisItemWindow .itemDetails .hd .xClosed {position: absolute;right: 0;font-size:40rpx;top:8rpx;z-index: 9;}
.thisItemWindow .itemDetails .hd .pic{position: absolute;top: -75rpx;height: 240rpx;width: 240rpx;border-radius: 10rpx;overflow: hidden;}
.thisItemWindow .itemDetails .hd .pic image{width: 100%;height: 100%}
.thisItemWindow .itemDetails .hd .itemDesc{position: relative;height: 165rpx;margin-left: 260rpx;padding-right:60rpx;}
.thisItemWindow .itemDetails .hd .itemDesc .h2{font-size: 28rpx;color: rgb(51,51,51);line-height: 1.5;margin-top: 10rpx}
.thisItemWindow .itemDetails .hd .itemDesc .h2{display: -webkit-box;display: -moz-box;overflow: hidden;text-overflow: ellipsis;word-break: break-all;-webkit-box-orient: vertical;-webkit-line-clamp:2;}
.thisItemWindow .itemDetails .hd .itemDesc .thisPrice{position: absolute;bottom: 0;left: 0;font-size: 24rpx;color: rgb(51,51,51);}
.thisItemWindow .itemDetails .hd .itemDesc .thisPrice em{color:rgb(255,56,103);font-size:36rpx  }
.thisItemWindow .itemDetails .itemAttrDesc{border-bottom: 1px solid #E8E8E8;padding: 10rpx 0}
.thisItemWindow .itemDetails .itemAttrDesc span{font-size: 24rpx;color: rgb(51,51,51);margin-right: 20rpx}
.thisItemWindow .itemDetails .itemAttrDesc span em{color: rgb(136,136,136);}

.thisItemWindow .itemAttrSet {margin-top: 55rpx;border-bottom: 1px solid #e8e8e8}
.thisItemWindow .itemAttrSet .attrSetBlock {margin-top: 30rpx}
.thisItemWindow .itemAttrSet .attrSetBlock .h3{font-size: 30rpx;color: rgb(51,51,51);line-height: 1;margin-bottom:25rpx;display: block;}
.thisItemWindow .itemAttrSet .attrSetBlock .attrBtn{overflow: hidden;margin-right: -25rpx}
.thisItemWindow .itemAttrSet .attrSetBlock .attrBtn span{float: left;min-width: 152rpx;height: 53rpx;text-align: center;font-size:26rpx;color: rgb(51,51,51);background: rgb(238,238,238);line-height: 53rpx;margin-right: 25rpx;margin-bottom: 25rpx;border-radius: 6rpx}
.thisItemWindow .itemAttrSet .attrSetBlock .attrBtn span.on{background: rgb(255,56,103);color: #fff}
.showTxt {color: #333;}
.showTxt image {display:inline-block;width:400rpx;height:400rpx;}
.thisItemWindow .itemNumSet {display:-moz-box;display:-webkit-box;display:box;padding: 30rpx 0}
.thisItemWindow .itemNumSet span{font-size:30rpx;color:rgb(51,51,51); }
.thisItemWindow .itemNumSet span, .thisItemWindow .itemNumSet .numCtrl{-moz-box-flex:1;-webkit-box-flex:1;box-flex:1}
.thisItemWindow .itemNumSet .numCtrl{text-align: right;}
.thisItemWindow .itemNumSet .numCtrl em{display: inline-block;width: 53rpx;height: 53rpx;vertical-align: middle;background-repeat: no-repeat!important;background-position: center center!important;background-color: #F0F0F1}
.thisItemWindow .itemNumSet .numCtrl input{vertical-align: middle;height: 53rpx;border: 0;background:#F0F0F1;color:  #000;display: inline-block;width: 72rpx;text-align: center;margin: 0 10rpx;font-size: 24rpx}
.thisItemWindow .itemNumSet .numCtrl em.reduce{background-image: url('data:image/png;base64,');background-size: 23rpx 4rpx}
.thisItemWindow .itemNumSet .numCtrl em.add{background-image: url('data:image/png;base64,');background-size: 24rpx 25rpx}

.presale .divLine {height: 96rpx}

.presale .ftBtn{position: fixed;width: 100%;height: 96rpx;line-height: 96rpx;color: #fff;font-size: 40rpx;text-align: center;bottom: 0;right: 0;left: 0;background: #FF3866;border-radius: 0;}
.presale .ftBtn.gray{background-color: #a6a6a6}

.presale .btnGroup{position: fixed;bottom: 0;right: 0;left: 0}
.presale .btnGroup .ftBtnChild {float: left;width: 50%;background-color: #FF9900;color: #fff;font-size: 40rpx;height: 96rpx;line-height: 96rpx;text-align: center;border-radius: 0;}
.payTime{
  font-size: 24rpx!important;
 
}

.presale .btnGroup .ftBtnChild:first-child {background-color: #FF3866;}
.presale .btnGroup .ftBtnChild.gray {background-color: #a6a6a6;}

.thisItemWindow .thisWindowBtn .ftBtnChild {display: block;width: 100%;height: 96rpx;line-height: 96rpx;color: #fff;font-size: 40rpx;text-align: center;border-radius: 0;}
.thisItemWindow .thisWindowBtn .ftBtnChild {background-color:#FF3866;}

