page{background-color: #f4f4f4}
.fr{float: right;}
.fl{float: left;}
.b_banner{height: 750rpx;width: 100%;position: relative;}
.b_banner image{height: 100%}
.b_banner span{position: absolute;left: 0;bottom: 40rpx;background-color: #fe6b31;color: #fff;padding: 0 10rpx;font-size: 22rpx;border-radius: 0 5rpx 5rpx 0;height: 38rpx;line-height: 38rpx}

.bargainIndex .list{position: absolute;top: 340rpx;width: 100%;bottom: 0}
.bargainIndex .list .li{margin-bottom: 68rpx}
.bargainIndex .list .li:last-child{margin-bottom: 34rpx}
.bargainIndex .list .wrap{padding: 0 30rpx}
.bargainIndex .list .pic{width: 265rpx;height: 265rpx;overflow: hidden;position: relative;border-radius: 5rpx;float: left;margin-right: 22rpx}
.bargainIndex .list .pic image{width: 100%;height: 100%}
.bargainIndex .list .pic span{position: absolute;left: 0;top: 10rpx;background-color: #fe6b31;color: #fff;padding: 10rpx 10rpx;font-size: 20rpx;line-height: 1;border-radius: 0 5rpx 5rpx 0}

.bargainIndex .list .desc{height: 265rpx;position: relative;overflow: hidden;}
.bargainIndex .list .desc .h3{font-size: 30rpx;color: #333;
	word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
    -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
    -webkit-line-clamp: 2; /** 显示的行数 **/
    overflow: hidden;  /** 隐藏超出的内容 **/}
.bargainIndex .list .desc p{display: block;}
.bargainIndex .list .desc .price_o{font-size: 26rpx;color: #999;margin-top: 10rpx}
.bargainIndex .list .desc .price_n{font-size: 36rpx;color: #ff3c32;margin-top: 10rpx}
.bargainIndex .list .desc .price_n span{font-size: 26rpx}
.bargainIndex .list .desc .btn{position: absolute;bottom: 0;width: 100%;background-color: #fe6b31;color: #fff;height: 55rpx;font-size: 28rpx;text-align: center;border-radius: 10rpx;line-height: 55rpx}



.bargainDetails .name{font-size: 34rpx;color: #333;background-color: #fff}
.bargainDetails .name .wrap{padding: 30rpx 24rpx}

.bargainDetails .numData{background-color: #fff;padding-top:60rpx;}
.bargainDetails .numData .wrap{padding: 30rpx 55rpx}
.bargainDetails .numData .progressBar{height: 23rpx;border:1px solid #fe6b31;background-color: #ffdacb;border-radius: 20rpx;position: relative;}
.bargainDetails .numData .progressBar .progressBar_inner{position: absolute;top: -1px;bottom: -1px;left: -1px;border-radius: 20rpx;}
.bargainDetails .numData .progressBar .progressBar_inner{
    background: -moz-linear-gradient(left, rgba(248,192,68,1) 0%, rgba(254,107,49,1) 100%);
    background: -webkit-linear-gradient(left, rgba(248,192,68,1) 0%,rgba(254,107,49,1) 100%);
    background: linear-gradient(to right, rgba(248,192,68,1) 0%,rgba(254,107,49,1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f8c044', endColorstr='#fe6b31',GradientType=1 );}
.bargainDetails .numData .progressBar .slider{width: 30rpx;height: 30rpx;border-radius: 100%;box-sizing: border-box;border:10rpx solid #fe6b31;position: absolute;top: -1px;right: -15rpx;background-color: #fff}
.bargainDetails .numData .progressBar p.current{position: absolute;top: -60rpx;left: 0%;font-size: 24rpx;color: #fe6b31;margin-left:-20rpx;white-space: nowrap;}
.bargainDetails .numData .progressBar p.current:before{content: " ";position: absolute;left: 0;top: 40rpx;color: #E5000D;border-top: 14rpx solid #fe6b31;border-right: 14rpx solid transparent;border-bottom: 14rpx solid transparent;
	border-left: 14rpx solid transparent;}
.bargainDetails .numData .progressBar p.current.on_right{left: inherit;}
.bargainDetails .numData .progressBar p.current.on_right:before{left: inherit;right: 0;}
.bargainDetails .numData .progressBar p.current em{font-size: 26rpx}
.bargainDetails .numData .price_p{font-size: 26rpx;color: #999;margin-top: 10rpx;display: block;}
.bargainDetails .numData .price_p .leftNum{text-align: center;display: block;font-size: 26rpx;color: #333}
.bargainDetails .numData .price_p .leftNum em{color: #ff3c32}

.bargainDetails .countDown{background-color: #fff;border-bottom: 1px solid #f2f2f2}
.bargainDetails .countDown .wrap{padding: 30rpx 24rpx;padding-top: 0}
.bargainDetails .countDown span,.bargainDetails .countDown em{display: inline-block;vertical-align: middle;}
.bargainDetails .countDown span{min-width: 45rpx;height: 45rpx;border-radius: 10rpx;line-height: 45rpx;text-align: center;background-color: #fe6b31;font-size: 30rpx;color: #fff}
.bargainDetails .countDown em{font-size: 26rpx;color: #666;margin: 0 10rpx}
.bargainDetails .countDown em.text{font-size: 28rpx}

.bargainDetails .descBlock{background-color: #ffd6ad;padding-bottom: 98rpx}
.bargainDetails .descBlock .wrap{padding: 46rpx 20rpx 20rpx}
.bargainDetails .descBlock .thisBlock{background-color: #fff;border:1px solid #ffa348;position: relative;}
.bargainDetails .descBlock .titBlock{text-align: center;top: -22rpx;display: block;padding: 0;margin: 0;position: absolute;left: 0;right: 0}
.bargainDetails .descBlock .titBlock span{display: inline-block;height: 44rpx;line-height: 44rpx;width: 150rpx;border:1px solid #ffa348;background-color: #ffd6ad;font-size: 26rpx;color: #333;border-radius: 50rpx}

.bargainDetails .richBlock {margin-top: 46rpx}
.bargainDetails .richBlock .titBlock span{margin: 0 20rpx}
.bargainDetails .richBlock .titBlock span.on{background-color: #ffa348;color: #fff}

.bargainDetails .descBlock .desc{padding: 60rpx 20rpx 20rpx}
.bargainDetails .richBlock .desc{font-size: 24rpx}
.bargainDetails .richBlock .desc image:nth-of-type(1){
  margin-top: 0;
}
.bargainDetails .richBlock .desc image{max-width: 100%;margin-top: -8rpx;}
.bargainDetails .richBlock .desc p{display: block;}
.bargainDetails .ruleBlock .desc p{display: block;font-size: 24rpx;color: #666;line-height: 1.5}
.bargainDetails .ruleBlock p.time{font-size: 26rpx;color: #999;display: block;padding: 0 20rpx;padding-bottom: 30rpx}
.bargainDetails .ruleBlock p.time i{color: #fe6b31;font-size: 26rpx;margin-right: 10rpx}

.bargainDetails .ftBtn{background-color: #fe6b31;font-size: 36rpx;color: #fff;height: 98rpx;line-height: 98rpx;text-align: center;position: fixed;bottom: 0;left: 0;right: 0;border-radius: 0;}
.bargainDetails .ftBtn.gray{background-color: #a6a6a6}
.bargainDetails .ftBtnGroup{background-color: #fff;position: fixed;bottom: 0;left: 0;right: 0}
.bargainDetails .ftBtnGroup .span,.bargainDetails .ftBtnGroup button{display: block;float: left;width: 50%;height: 98rpx;text-align: center;background-color: #fe6b31;line-height: 98rpx;font-size: 36rpx;color: #fff;border-radius: 0;border:0;}
.bargainDetails .ftBtnGroup button{background-color: #f39800;color: #fff}
.bargainDetails .ftBtnGroup button::after {display: none}
.bargainDetails .ftBtnGroup button.gray{background-color: #a6a6a6}

.bargainDetails .teamBlock{margin-top: 46rpx}
.bargainDetails .teamBlock .group .hd{padding:40rpx 20rpx}
.bargainDetails .teamBlock .group .hd em{color: #999;font-size: 26rpx}
.bargainDetails .teamBlock .group .hd p{color: #333;font-size: 26rpx;}
.bargainDetails .teamBlock .group .bd{padding-bottom: 40rpx}
.bargainDetails .teamBlock .group .bd .ul .li{float: left;width: 98rpx;height: 98rpx;position: relative;margin:0 21.8rpx;
}
.bargainDetails .teamBlock .group .bd .ul .li image{width: 98rpx;height: 98rpx;border-radius: 100%}
.bargainDetails .teamBlock .group .bd .ul .li p{position: absolute;bottom: 0;border:1px solid #dbdbdb;border-radius: 20rpx;font-size: 20rpx;color: #fe6b31;display: block;left: 0;right: 0;background-color: #fff;text-align: center;}

.bargainDetails .teamBlock .group .bd .more{float: left;width: 98rpx;height: 98rpx;margin:0 21.8rpx;}
.bargainDetails .teamBlock .group .bd .more{background:url('data:image/png;base64,');background-size: cover;
}

.mask{z-index:20;position:fixed;top:0;right:0;bottom:0;left:0;width:100%;background-color:#000;opacity:.7;filter:alpha(opacity=80);-moz-opacity:.8;-khtml-opacity:.8;}
.bargainDetails .teamList {position: fixed;bottom: 0;left: 0;right: 0;z-index: 30;background-color: #fff;border-radius: 30rpx 30rpx 0 0;overflow: hidden;}
.bargainDetails .teamList .h2{font-size: 32rpx;color: #333;text-align: center;display: block;background-color: #f4f4f4;height: 100rpx;line-height: 100rpx}
.bargainDetails .teamList .wrap{padding: 0 30rpx}
.bargainDetails .teamList span.iconfont{position:absolute;right:35rpx;font-size:36rpx;top:30rpx;}
.bargainDetails .teamList .t_list{height: 670rpx;overflow: auto;}
.bargainDetails .teamList .t_list .li{padding: 20rpx 0;border-bottom: 1px dashed #e6e6e6}
.bargainDetails .teamList .t_list .li:last-child{border-bottom: 0}
.bargainDetails .teamList .t_list image{width: 100rpx;height: 100rpx;border-radius: 100%;margin-right: 20rpx}
.bargainDetails .teamList .t_list .name{font-size: 30rpx;color: #333;display: block;padding-top: 12rpx}
.bargainDetails .teamList .t_list .time{font-size: 26rpx;color: #999;display: block;line-height: 2}
.bargainDetails .teamList .t_list span.cut{color:#fe6b31;font-size: 28rpx;line-height: 100rpx}

.bargainDetails .resultWindow {position: fixed;height: 720rpx;width: 610rpx;background-color: #fff;top: 50%;margin-top: -360rpx;left: 50%;margin-left: -305rpx;z-index: 21;text-align: center;border-radius: 10rpx}
.bargainDetails .resultWindow .wrap{padding: 0 50rpx}
.bargainDetails .resultWindow .h2{font-size: 38rpx;color: #333;text-align: center;line-height: 1;margin-top: 60rpx}
.bargainDetails .resultWindow .h2 .price_span{color: #fa5e4f;margin-top: 8rpx;}
.bargainDetails .resultWindow .h3{font-size: 26rpx;color: #5b667a;text-align: center;display: block;margin-top: 25rpx}
.bargainDetails .resultWindow image{width: 393rpx;height: 336rpx;margin-top: 40rpx}

.bargainDetails .resultWindow .shareBtn{width: 100%;height: 88rpx;text-align: center;background-color: #fe6b31;font-size: 34rpx;color:#fff;line-height: 88rpx;border-radius: 10rpx;margin-top: 20rpx}
.bargainDetails .resultWindow .shareBtn::after {display: none;}

