  0% {
   color: red;
   bottom: -5rpx;
  }
  50% {
    color: orange;
    bottom: 0rpx;
  }
  100% {
    color: red;
    bottom: -5rpx;
  }
}
.flag-hide{
  display: none!important;
}
.flag-tap{
  font-size: 22px;
  position: absolute;
  bottom: -5rpx;
  right: 15rpx;
  animation: viewlinear 2s linear infinite;
  display: block!important;
}

